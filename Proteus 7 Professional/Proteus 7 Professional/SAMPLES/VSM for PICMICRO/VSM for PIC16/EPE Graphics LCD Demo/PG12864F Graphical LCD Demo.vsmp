<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<PROJECT DESCRIPTION="Using a PIC16F877 processor to control a PG12864F Graphical LCD Display.">
 <KEYWORDS>
  <KEYWORD NAME="LCD Control"/>
 </KEYWORDS>
 <FILES>
  <FILE GROUP="Source" NAME="GEPE456.ASM"/>
  <FILE GROUP="Design" NAME="GEPE456.DSN"/>
 </FILES>
 <CONFIGURATION TARGET="Debug" COMPILER="MPASM (Proteus)" NAME="Debug">
  <CONTROLLER FAMILY="PIC16" CLOCK="4MHz" TYPE="PIC16F877"/>
  <FILE TOOL="LD" NAME="Debug.cof">
   <OPTION DEFAULT="1" TOOL="LD" ID="OUTFILE" NAME="-o " VALUE="%OUTFILE_DIR%/%OUTFILE_NAME%.cof"/>
   <OPTION DEFAULT="1" TOOL="LD" ID="LKR" NAME="" VALUE="16f877_g.lkr"/>
   <FILE TOOL="ASM" NAME="GEPE456.o">
    <FILE NAME="GEPE456.ASM"/>
   </FILE>
  </FILE>
  <FILE NAME="GEPE456.DSN"/>
  <OPTION DEFAULT="1" TOOL="LD" NAME="-x"/>
  <OPTION DEFAULT="1" TOOL="ASM" NAME="/q" VALUE="+"/>
  <OPTION DEFAULT="1" TOOL="ASM" NAME="/p" VALUE="16f877"/>
 </CONFIGURATION>
</PROJECT>
